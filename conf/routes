# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET    /registro                    controllers.HomeController.registro
GET    /registro/inicio             controllers.HomeController.iniciarRegistro(tipoRegistro: TipoRegistro)

->  /kilometraje kilometraje.Routes

GET /prestamo/iniciar               controllers.HomeController.prestamoIniciar
GET /prestamo/finalizar             controllers.HomeController.finalizarPrestamo
GET /gamificationBiciGov            controllers.HomeController.gamificationBiciGov
GET /gamificationBiciCity            controllers.HomeController.gamificationBiciCity
GET /redimir                        controllers.HomeController.catalogoPremios

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)